J(2,1) = 0 ;
J(2,2) = 0 ;
J(2,3) = 0 ;
J(3,1) = 0 ;
J(3,2) = 0 ;
J(3,3) = 0 ; 
elseif nx(2)==2 % second derivative wrt J(t-par(7)) and A(t-par(7))
J(1,1) = 0 ;
J(1,2) = 0 ;
J(1,3) = 0;
J(2,1) = 0 ;
J(2,2) = 0 ;
J(2,3) = 0 ;
J(3,1) = 0 ;
J(3,2) = 0 ;
J(3,3) = 0 ; 
elseif nx(2)==3 % second derivative wrt J(t-par(7)) and A(t-par(7))
J(1,1) = 0 ;
J(1,2) = 0 ;
J(1,3) = 0 ;
J(2,1) = v(3)*((par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2) - (xx(3,4)*v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - (xx(3,4)*v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 ;
J(2,2) = v(3)*((par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2) - (xx(3,4)*v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - (xx(3,4)*v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 ;
J(2,3) = v(3)*((2*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2) + (v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) + (v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (xx(3,4)*v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - (xx(3,4)*v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 ;
J(3,1) = (xx(3,4)*v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - v(3)*((par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2) + (xx(3,4)*v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 ;
J(3,2) = (xx(3,4)*v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - v(3)*((par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2) + (xx(3,4)*v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 ;
J(3,3) = (xx(3,4)*v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - (v(1)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - v(3)*((2*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5) - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2) + (xx(3,4)*v(2)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 ; 
end 
end
% else
% J=df_deriv(struct('sys_rhs',f),xx,par,nx,np,v);
end
%% Otherwise raise error
% Raise error if the requested derivative does par(4)t exist
if isempty(J)
error(['SYS_DERI: requested derivative nx=%d, np=%d, size(v)=%d',...
'could par(4)t be computed!'],nx,np,size(v));
end
end
