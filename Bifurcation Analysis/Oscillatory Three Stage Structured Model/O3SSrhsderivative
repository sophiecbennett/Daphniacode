J(3,2) = (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6)))*(xx(3,4) + xx(1,4) + xx(2,4)))/par(5)^3 ;
J(3,3) = (par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6)))*(xx(3,4) + xx(1,4) + xx(2,4)))/par(5)^2 + (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6))))/par(5)^2 - (xx(3,4)*par(1)*exp(-par(7)*par(2))*exp(-par(4)*par(8))*exp(-(xx(3,4) + xx(1,4) + xx(2,4))/par(5))*exp(par(3)*(par(7) - par(6)))*(xx(3,4) + xx(1,4) + xx(2,4)))/par(5)^3 ; 
